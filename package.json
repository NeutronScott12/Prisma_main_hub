{
	"name": "prisma-backend",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "cross-env NODE_ENV=development && nodemon -e ts,graphql -x ts-node src/index.ts",
		"dev": "cross-env NODE_ENV=development && npm-run-all --parallel start playground",
		"debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
		"playground": "graphql playground",
		"copy-graphql": "copyfiles -u 1 src/**/*.graphql dist",
		"delete-build": "rimraf ./dist",
		"build": "npm run delete-build && tsc && npm run copy-graphql",
		"gen-schema-types": "ts-node src/scripts/createTypes.ts",
		"prisma-deploy": "prisma deploy",
		"prisma-up": "docker-compose up -d",
		"prisma-down": "docker-compose down",
		"seed": "ts-node database/seed.ts"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
